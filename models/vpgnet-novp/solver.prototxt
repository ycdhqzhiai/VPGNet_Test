net: "./train_val.prototxt"

test_iter: 20
test_interval: 100
test_compute_loss: true

base_lr: 0.005
lr_policy: "step"
gamma: 0.1
stepsize: 100000 # default 100000
display: 10
max_iter: 100000 # default 100000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "./snapshots/split"
solver_mode: GPU
